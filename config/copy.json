{
  "app": {
    "expand": true,
    "cwd": "<%= config.app %>",
    "src": "**/*.html",
    "dest": "<%= config.dist %>/"
  },
  "assets": {
    "expand": true,
    "cwd": "<%= config.app %>/assets",
    "src": "**/*",
    "dest": "<%= config.dist %>/assets"
  },
  "framework": {
    "src": "<%= config.framework %>/index.html",
    "dest": "<%= config.dist %>/index.html"
  }
}
